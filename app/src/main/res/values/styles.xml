<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CircularProgressBar">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">@dimen/v3_progress_bar_margin_top</item>
        <item name="android:layout_marginLeft">@dimen/v3_progress_bar_margin_left</item>
        <item name="android:layout_marginRight">@dimen/v3_progress_bar_margin_left</item>
        <item name="stroke_width">@dimen/v3_progress_bar_width</item>
        <item name="progress_color">@color/dark_blue</item>
        <item name="progress_background_color">@color/progress_back_outside</item>
        <item name="second_visible">true</item>
        <item name="second_progress_color">@color/light_blue</item>
        <item name="second_progress_background_color">@color/progress_back_inside</item>
    </style>

</resources>
